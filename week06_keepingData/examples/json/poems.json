{
    "poems": [
        {
            "title": "晓出净慈寺送林子方",
            "author": {
                "name": "杨万里",
                "dynasty": "宋"
            },
            "verses": [
                {
                    "verse": "毕竟西湖六月中",
                    "end": "，"
                },
                {
                    "verse": "风光不与四时同",
                    "end": "。"
                },
                {
                    "verse": "接天莲叶无穷碧",
                    "end": "，"
                },
                {
                    "verse": "映日荷花别样红",
                    "end": "。"
                }
            ]
        },
        {
            "title": "登高",
            "author": {
                "name": "杜甫",
                "dynasty": "唐"
            },
            "verses": [
                {
                    "verse": "风急天高猿啸哀",
                    "end": "，"
                },
                {
                    "verse": "渚清沙白鸟飞回",
                    "end": "。"
                },
                {
                    "verse": "无边落木萧萧下",
                    "end": "，"
                },
                {
                    "verse": "不尽长江滚滚来",
                    "end": "。"
                },
                {
                    "verse": "万里悲秋常作客",
                    "end": "，"
                },
                {
                    "verse": "百年多病独登台",
                    "end": "。"
                },
                {
                    "verse": "艰难苦恨繁霜鬓",
                    "end": "，"
                },
                {
                    "verse": "潦倒新停浊酒杯",
                    "end": "。"
                }
            ]
        }
    ]
}
